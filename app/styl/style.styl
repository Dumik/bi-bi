	transition: 0.5s

.btn-feedback:hover
	background: #900000
	border 2px solid #A3121B
	float: right
	padding 15px 80px
	transition: 0.5s



.form-control:focus
	border 1px solid #000
	box-shadow: 0 0 0 rgba(0,0,0,0.5)



.feedback-result 
	width: 100%
	height auto
	display: none

.feedback-result p    
	padding: 10px
	color: #ffffff
	background: #a3121b


.contact 
	width 100%
	height auto
