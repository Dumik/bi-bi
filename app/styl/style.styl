	font-size: 32px
	font-weight: 700

.container-contect 
	display: grid
	grid-template-columns: 1fr 1fr 1fr 1fr
	grid-template-rows: 1

.container-contect p     
	padding: 20px 0
	font-family: 'Montserrat', sans-serif;
	font-size: 20px
	font-weight: 700



/*----------response------*/
.response
	width: 100%
	height: auto
	background: #A3121B
	display flex
	flex-wrap: nowrap
	flex-direction:  row
	align-items: center
	justify-content: center



.response-countainer
	width: 100%
	height: 500px
	margin: 100px 0 
	padding: 0
	background: #fff


.carousel
	width: 100%
	height: 500px

	
.carousel-item-next, .carousel-item-prev, .carousel-item.active
	display: flex

.carousel-item
	display flex
	flex-wrap: nowrap
	flex-direction:  row
	align-items: center	
	justify-content: space-between


.response-info 
	width: 100%
	height: 500px
	display flex
	flex-wrap: nowrap
	flex-direction:  row
	align-items: center	
	justify-content: center

.response-text
	width: 100%
	height: auto

.response-text   
	font-family: 'Montserrat', sans-serif;
	font-size: 16px
	font-weight: 500
	line-height: 24px
	color: #000000
	padding 0 0 30px 0


.response-signature 
	text-align: center
	font-family: 'Montserrat', sans-serif;
	font-size: 24px
	font-weight: 700
	line-height: 32px
	color: #000000
	
.carousel-indicators li {
    box-sizing: content-box;
    -ms-flex: 0 1 auto;
    flex: 0 1 auto;
    width: 15px;
    height: 15px;
    margin-right: 3px;
    margin-left: 3px;
    text-indent: -999px;
    cursor: pointer;
    background-color: #A3121B;
    background-clip: padding-box;
    border-top: 10px solid transparent;
    border-bottom: 10px solid transparent;
    opacity: .5;
    transition: opacity .6s ease;
}

.carousel-item::before
	width: 70px
	height: 70px
	margin: -150px 150px 50px -450px
	content: " ."
	background:  url('../img/quote.svg') center no-repeat;
	background-size: cover;
	position absolute



