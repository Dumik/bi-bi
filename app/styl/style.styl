	font-size: 56px
	font-weight: 400
	letter-spacing: 1.2px
	margin-bottom: 50px
	color #fff

.form-row>[class*=col-]
	padding: 0

.form-control
	border-radius: 2px
	border 1px solid #000
	padding: 25px 10px
.form-group
	padding: 0 0 0 10px
	margin 0 0 10px 0



.btn-feedback
	background: #A3121B
	color: #fff
	border-radius: 2px
	border 2px solid #A3121B
	float: right
	padding 15px 50px
	font-family: 'Montserrat', sans-serif;
	font-size: 18px
