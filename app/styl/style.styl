	text-align: center

.my-modal-title h4
	font-family: 'Montserrat', sans-serif;
	font-size: 48px
	font-weight: 400
	letter-spacing: 1.2px
	line-height: 100px

.my-modal-body
	width 100%
	height auto
	padding-bottom: 20px
	display flex
	flex-wrap: nowrap
	flex-direction:  row
	justify-content: space-around


.countainer-modal-img
	width 100%
	height auto

.countainer-modal-img img   
	width: 100%
	height auto


.countainer-modal-text 
	text-align:  justify

.countainer-modal-text p
